    "@rollup/plugin-node-resolve": "^13.0.5",
    "@ryan-liu/eslint-config-jsx-config": "^1.0.2",
    "@types/qs": "^6.9.7",
    "eslint": "^7.32.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "rollup": "^2.57.0",
